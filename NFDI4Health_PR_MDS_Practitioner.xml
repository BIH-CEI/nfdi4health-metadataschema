<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="nfdi4health-pr-mds-practitioner" />
  <url value="https://www.nfdi4health.de/fhir/metadataschema/StructureDefinition/nfdi4health-pr-mds-practitioner" />
  <version value="0.9" />
  <name value="NFDI4Health_PR_MDS_Practitioner" />
  <title value="NFDI4Health PR MDS Practitioner" />
  <status value="draft" />
  <publisher value="NFDI4Health" />
  <contact>
    <telecom>
      <system value="url" />
      <value value="https://www.nfdi4health.de" />
    </telecom>
  </contact>
  <description value="Resource covering metadata of an individual." />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Practitioner" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Practitioner" />
  <derivation value="constraint" />
  <differential>
    <element id="Practitioner">
      <path value="Practitioner" />
      <short value="Details about the person" />
      <definition value="Group of items providing information about person(s) contributing to the resource." />
      <comment value="Additional information: Group of items applicable to personal names." />
      <mapping>
        <identity value="NFDI4Health" />
        <map value="1.11.3 Resource.roles.personal" />
      </mapping>
    </element>
    <element id="Practitioner.identifier">
      <path value="Practitioner.identifier" />
      <short value="Digital identifier(s) of the person" />
      <definition value="Group of items providing information about digital identifiers of the person." />
      <mapping>
        <identity value="NFDI4Health" />
        <map value="1.11.3.5 Resource.roles.personal.identifiers" />
      </mapping>
    </element>
    <element id="Practitioner.identifier.type">
      <path value="Practitioner.identifier.type" />
      <short value="Which type of identifer is reported?" />
      <definition value="Type of the identifier scheme." />
      <min value="1" />
      <example>
        <label value="Example of identifier scheme" />
        <valueCodeableConcept>
          <text value="ORCID" />
        </valueCodeableConcept>
      </example>
      <binding>
        <strength value="required" />
        <description value="Value set defining codes to specify the identifier scheme of a personal identifier." />
        <valueSet value="https://www.nfdi4health.de/fhir/metadataschema/ValueSet/nfdi4health-vs-mds-role-name-identifier-scheme" />
      </binding>
      <mapping>
        <identity value="NFDI4Health" />
        <map value="1.11.3.5.2 Resource.roles.personal.identifiers.scheme" />
      </mapping>
    </element>
    <element id="Practitioner.identifier.type.coding">
      <path value="Practitioner.identifier.type.coding" />
      <min value="1" />
      <max value="1" />
      <binding>
        <strength value="required" />
        <description value="Value set defining codes to specify the identifier scheme of a personal identifier." />
        <valueSet value="https://www.nfdi4health.de/fhir/metadataschema/ValueSet/nfdi4health-vs-mds-role-name-identifier-scheme" />
      </binding>
    </element>
    <element id="Practitioner.identifier.value">
      <path value="Practitioner.identifier.value" />
      <short value="Identifier" />
      <definition value="Digital identifier of the person according to a definite scheme." />
      <comment value="Short input help: Preferably, provide an ORCID (https://orcid.org/)." />
      <min value="1" />
      <example>
        <label value="Example of ORCID" />
        <valueString value="0000-0003-1379-7023" />
      </example>
      <mapping>
        <identity value="1.11.3.5" />
        <map value="identifier" />
      </mapping>
    </element>
    <element id="Practitioner.name">
      <path value="Practitioner.name" />
      <min value="1" />
      <max value="1" />
    </element>
    <element id="Practitioner.name.family">
      <path value="Practitioner.name.family" />
      <short value="Family name of the person" />
      <definition value="Family name of the person" />
      <min value="1" />
      <mapping>
        <identity value="NFDI4Health" />
        <map value="1.11.3.3 Resource.roles.personal.familyName" />
      </mapping>
    </element>
    <element id="Practitioner.name.given">
      <path value="Practitioner.name.given" />
      <short value="Given name of the person" />
      <definition value="Given name of the person." />
      <min value="1" />
      <max value="1" />
      <mapping>
        <identity value="NFDI4Health" />
        <map value="1.11.3.2 Resource.roles.personal.givenName" />
      </mapping>
    </element>
    <element id="Practitioner.name.prefix">
      <path value="Practitioner.name.prefix" />
      <short value="Title of the person" />
      <definition value="Title of the person." />
      <min value="1" />
      <max value="1" />
      <binding>
        <strength value="required" />
        <description value="Value set defining codes to specify personal titles." />
        <valueSet value="https://www.nfdi4health.de/fhir/metadataschema/ValueSet/nfdi4health-vs-mds-role-name-personal-title-nci" />
      </binding>
      <mapping>
        <identity value="NFDI4Health" />
        <map value="1.11.3.4 Resource.roles.personal.title" />
      </mapping>
    </element>
  </differential>
</StructureDefinition>